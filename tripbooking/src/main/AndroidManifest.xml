<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.richie.tripbooking" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/listcart"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SpashScreen"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".Login_User"
            android:label="@string/title_activity_login__user" >
        </activity>
        <activity
            android:name=".BookingTrip"
            android:label="@string/title_activity_booking_trip" >
        </activity>
        <activity
            android:name=".Drive_Selection"
            android:label="Choose Package" >

        </activity>
        <activity
            android:name=".PersonalDetails"
            android:label="Personal Details" >
        </activity>
        <activity
            android:name=".DateOnway"
            android:label="@string/title_activity_date_onway" >
        </activity>
        <activity
            android:name=".Cities"
            android:label="@string/title_activity_cities" >
        </activity>
        <activity
            android:name=".PersonalDetail"
            android:label="@string/title_activity_personal_detail" >
        </activity>
        <activity
            android:name=".Booking_Details"
            android:label="@string/title_activity_booking__details" >
        </activity>
    </application>

</manifest>
